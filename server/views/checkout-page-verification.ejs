        <%- include('partials/navbar.ejs')%>

    <div class="container">
        <div class="row gy-3 gx-5 products text-center">
    
    
<% if(cart.length > 0){%>
    <%for(let i = 0; i< cart.length; i++){%>
              <div class="col-md-3 col-lg-4 product" id=<%=cart[i].product._id %> >
    <div class="card" >
      <img src='<%= cart[i].product.mainImage %>' class="card-img-top" alt="..." />
      <div class="card-body">
        <h5 class="card-title"></h5>
        <h2><%=cart[i].product.productName %></h2>
        <h3>$<%= cart[i].product.price%></h3>
        <h3>Option: <%= cart[i].option %> </h3>
        <p class="card-text"></p>
        <button class="btn btn-danger remove-from-cart">Remove From Cart</button>
      </div>
    </div>

    </div>
<%}} else {%>
<h1>No Items</h1>
<% }%>


<% if(cart.length > 0){%>
    <% let price = 0 %>
    <% const getPrice = () => { %>
    <% for(let i = 0; i < cart.length; i++){ %>
        
       <% price += Number(cart[i].product.price) %>
       
        <%}%>
    <%return price;%>
    <%}%>


    <div id='total'><h2>Total: $<%=getPrice() %></h2> </div>

<%} else {%>
    <div id='total'><h2>Total: 0</h2> </div>

    <%}%>
    
    <p style="color: red;"> All credit card payments are processed through a secure and encrypted 256-bit SSL checkout using Square&reg. Our team will never see, or store any credit card information under any circumstances. 
      Billing and Delivery address must be the same or the payment will not be accepted.
    </u> For bitcoin payments only delivery information and email is required, billing can be left blank in this case. 
    </p>

